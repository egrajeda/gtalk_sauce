{
    "name": "Sauce for Google Talk",
    "version": "0.1.1",
    "manifest_version": 2,
    "description": "Some small enhacements for the Gmail's embedded Google Talk client.",
    "content_scripts": [
        {
            "matches": ["*://mail.google.com/mail*"],
            "js": ["jquery.js", "extension.js"],
            "css": ["extension.css"],
            "run_at": "document_start",
            "all_frames": false
        }
    ]
}
